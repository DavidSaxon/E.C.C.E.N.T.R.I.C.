<h1>Listing items</h1>
 
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
    <th>Delete</th>
  </tr>
 
  <% @random.each do |something| %>
    <tr>
      <td><%= something.name %></td>
      <td><%= image_tag something.url, class: "dragme", id: "item" %></td>
      <td><%= button_to "delete", something, :method=>:delete, :class=>:destroy, :confirm => 'Are you sure?' %></td>
    </tr>
  <% end %>
</table>