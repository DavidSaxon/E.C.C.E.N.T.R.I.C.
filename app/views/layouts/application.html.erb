<!DOCTYPE html>
<html>
<head>
<title>ECCENTRIC</title>
<%= javascript_include_tag "application" %>
<%= javascript_include_tag "http://code.jquery.com/ui/1.9.2/jquery-ui.js" %>
<%= javascript_include_tag "//netdna.bootstrapcdn.com/bootstrap/3.0.0/js/bootstrap.min.js" %>


<%= stylesheet_link_tag"http://code.jquery.com/ui/1.9.2/themes/base/jquery-ui.css" %>

<!-- Latest compiled and minified CSS -->
<%= stylesheet_link_tag "//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap.min.css" %>

<!-- Optional theme -->
<%= stylesheet_link_tag "//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap-theme.min.css" %>

<%= stylesheet_link_tag "bootstrap_all.css" %>

<%= csrf_meta_tags %>
</head>
<body>

    <div class="navbar navbar-inverse navbar-fixed-top">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="#">ECCENTRIC</a>
        </div>
        <div class="collapse navbar-collapse">
          <ul class="nav navbar-nav">
            <li class="active"><%= link_to 'Home', root_url %></li> <!--<a href="#">Home</a>-->
            <li><%= link_to 'Simulation', simulations_url %></li>
            <li><%= link_to 'Display', displays_url %></li>
            <li><%= link_to 'Users', users_url %></li>
            <li><%= link_to 'Settings', settings_url %></li>
            <li><a href="#about">About</a></li>
            <li><a href="#contact">Contact</a></li>
          </ul>
        </div><!--/.nav-collapse -->
      </div>
    </div>

<meta name="viewport" content="width=device-width, initial-scale=1.0">

<%= yield %>

</body>
</html>
